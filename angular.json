{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api-interfaces/**/*.ts"]
          }
        }
      }
    },
    "catalog-domain": {
      "projectType": "library",
      "root": "libs/catalog/domain",
      "sourceRoot": "libs/catalog/domain/src",
      "prefix": "catalog",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/catalog/domain"],
          "options": {
            "project": "libs/catalog/domain/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/catalog/domain/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/catalog/domain/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/catalog/domain"],
          "options": {
            "jestConfig": "libs/catalog/domain/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/catalog/domain/src/**/*.ts",
              "libs/catalog/domain/src/**/*.html"
            ]
          }
        }
      }
    },
    "catalog-feature-list": {
      "projectType": "library",
      "root": "libs/catalog/feature-list",
      "sourceRoot": "libs/catalog/feature-list/src",
      "prefix": "catalog",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/catalog/feature-list"],
          "options": {
            "project": "libs/catalog/feature-list/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/catalog/feature-list/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/catalog/feature-list/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/catalog/feature-list"],
          "options": {
            "jestConfig": "libs/catalog/feature-list/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/catalog/feature-list/src/**/*.ts",
              "libs/catalog/feature-list/src/**/*.html"
            ]
          }
        }
      }
    },
    "light-box-domain": {
      "projectType": "library",
      "root": "libs/light-box/domain",
      "sourceRoot": "libs/light-box/domain/src",
      "prefix": "light-box",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/light-box/domain"],
          "options": {
            "project": "libs/light-box/domain/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/light-box/domain/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/light-box/domain/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/light-box/domain"],
          "options": {
            "jestConfig": "libs/light-box/domain/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/light-box/domain/src/**/*.ts",
              "libs/light-box/domain/src/**/*.html"
            ]
          }
        }
      }
    },
    "light-box-feature-add-to-light-box-button": {
      "projectType": "library",
      "root": "libs/light-box/feature-add-to-light-box-button",
      "sourceRoot": "libs/light-box/feature-add-to-light-box-button/src",
      "prefix": "light-box",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/light-box/feature-add-to-light-box-button"],
          "options": {
            "project": "libs/light-box/feature-add-to-light-box-button/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/light-box/feature-add-to-light-box-button/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/light-box/feature-add-to-light-box-button/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/light-box/feature-add-to-light-box-button"
          ],
          "options": {
            "jestConfig": "libs/light-box/feature-add-to-light-box-button/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/light-box/feature-add-to-light-box-button/src/**/*.ts",
              "libs/light-box/feature-add-to-light-box-button/src/**/*.html"
            ]
          }
        }
      }
    },
    "sales-app": {
      "projectType": "application",
      "root": "apps/sales-app",
      "sourceRoot": "apps/sales-app/src",
      "prefix": "sales-app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/sales-app",
            "index": "apps/sales-app/src/index.html",
            "main": "apps/sales-app/src/main.ts",
            "polyfills": "apps/sales-app/src/polyfills.ts",
            "tsConfig": "apps/sales-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/sales-app/src/favicon.ico",
              "apps/sales-app/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/sales-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/sales-app/src/environments/environment.ts",
                  "with": "apps/sales-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "sales-app:build:production"
            },
            "development": {
              "browserTarget": "sales-app:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/sales-app/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sales-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/sales-app/src/**/*.ts",
              "apps/sales-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/sales-app"],
          "options": {
            "jestConfig": "apps/sales-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "sales-app-e2e": {
      "root": "apps/sales-app-e2e",
      "sourceRoot": "apps/sales-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/sales-app-e2e/cypress.json",
            "tsConfig": "apps/sales-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "sales-app:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sales-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/sales-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "sales-app"
}
